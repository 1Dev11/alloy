'#index': {
	backgroundColor: '#fff',
	fullscreen: false,
	exitOnClose: true
}
'#lbl': {
	text: 'Alloy.Globals.foo is false',
	color: 'black'
}
'#lbl[if=Alloy.Globals.foo]': {
	text: 'Alloy.Globals.foo is true',
	font: {
		fontSize: 24
	}
}
'#lbl[if=Alloy.Globals.isIOS7]': {
	top: 50
}
'#lbl[if=Alloy.Globals.isTalliPhone]': {
	backgroundColor: 'yellow'
}
'#lbl[formFactor=handheld]': {
	text: 'Handheld and Alloy.Globals.foo false',
	color: 'blue'
}
'#lbl[if=Alloy.Globals.foo formFactor=tablet]': {
	text: 'Alloy.Globals.foo is true, and this is a tablet',
	color: 'green',
	font: {
		fontWeight: 'bold',
		fontSize: 40,
		fontFamily: "Cochin-BoldItalic"
	}
}
"#iPhone5Label": {
	text: 'iPhone5'
}
"#pre989label": {
	bottom: 10
}
"#pre989label[platform=ios,android formFactor=tablet]": {
	text: "Tablet (existing TSS functionality)"
}
"#pre989label[platform=ios,android formFactor=handheld]": {
	text: "Handheld (existing TSS functionality)"
}
